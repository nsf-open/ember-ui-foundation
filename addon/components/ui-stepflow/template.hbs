<UiProgressBar @manager={{this.manager}} />

<UiPanel
  @heading="{{this.heading}}"
  @variant="{{this.variant}}"
  @testId={{this.testId}}
>
  {{component this.manager.currentStep.component
    progressItem    = this.manager.currentStep
    progressManager = this.manager
    progressData    = this.manager.data
  }}
</UiPanel>

{{ui-stepflow/navbar
  manager=this.manager
  submitButtonText=this.submitButtonText
  cancellationRoute=this.cancellationRoute
  testId=this.testId
  completeStepFlow=this.completeStepFlow
}}

{
  "name": "@nsf/ui-foundation",
  "version": "1.2.0",
  "description": "The starting point for Ember Application UI libraries at NSF.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://bitbucket.nsf.gov/projects/NSF-FE/repos/ui-foundation/browse",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "start:with-storybook": "STORYBOOK=true npm-run-all --continue-on-error --parallel start storybook:start",
    "test:all": "npm-run-all lint test:*",
    "test": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepack": "ember ts:precompile",
    "postpack": "ember ts:clean",
    "release": "standard-version",
    "storybook:build": "ember build && build-storybook -s dist",
    "storybook:start": "start-storybook -p 6006 -s dist --ci"
  },
  "dependencies": {
    "@ember-decorators/component": "^6.1.1",
    "@nsf/general-utils": "^1.1.1",
    "@popperjs/core": "^2.10.2",

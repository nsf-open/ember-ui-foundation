{
  "name": "@nsf-open/ember-ui-foundation",
  "version": "1.8.0",
  "description": "The starting point for Ember Application UI libraries at NSF.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/nsf-open/ember-ui-foundation",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "start:with-storybook": "STORYBOOK=true npm-run-all --continue-on-error --parallel start storybook:start",
    "test:all": "npm-run-all lint test:*",
    "test": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test:coverage": "COVERAGE=true ember test",
    "prepack": "ember ts:precompile",
    "postpack": "ember ts:clean",
    "release": "standard-version",
    "storybook:build": "STORYBOOK=true ember build && build-storybook -o ./docs",
    "storybook:start": "sleep 30s && start-storybook -p 6006 --ci"
  },
  "dependencies": {
    "@ember-decorators/component": "6.1.1",
    "@nsf-open/ember-general-utils": "2.0.1",
    "@popperjs/core": "2.11.5",
    "broccoli-funnel": "3.0.8",
    "ember-auto-import": "2.4.2",
    "ember-cli-babel": "7.26.11",
    "ember-cli-htmlbars": "6.0.1",
    "ember-cli-typescript": "4.2.1",
    "ember-concurrency": "2.2.1",
    "ember-concurrency-ts": "0.3.1",
    "ember-element-helper": "0.5.5",
    "ember-in-element-polyfill": "1.0.1",
    "ember-truth-helpers": "3.0.0",
    "font-awesome": "4.7.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.18.2",
    "@babel/eslint-plugin": "7.17.7",
    "@ember/optional-features": "2.0.0",
    "@ember/test-helpers": "2.8.1",
    "@embroider/test-setup": "0.50.2",
    "@glimmer/component": "1.0.4",
    "@glimmer/tracking": "1.0.4",
    "@storybook/addon-a11y": "6.5.9",
    "@storybook/addon-cssresources": "6.2.9",
    "@storybook/addon-essentials": "6.5.9",
    "@storybook/ember": "6.5.9",
    "@storybook/ember-cli-storybook": "0.6.0",
    "@storybook/theming": "6.5.9",
    "@types/ember__application": "3.16.4",
    "@types/ember__array": "3.16.5",
    "@types/ember__component": "3.16.7",
    "@types/ember__controller": "3.16.7",
    "@types/ember__debug": "3.16.7",
    "@types/ember__engine": "3.16.4",
    "@types/ember__error": "3.16.2",
    "@types/ember__object": "3.12.8",
    "@types/ember__polyfills": "3.12.2",
    "@types/ember__routing": "3.16.17",
    "@types/ember__runloop": "3.16.4",
    "@types/ember__service": "3.16.2",
    "@types/ember__string": "3.16.3",
    "@types/ember__template": "3.16.2",
    "@types/ember__test": "3.16.2",
    "@types/ember__test-helpers": "2.6.1",
    "@types/ember__utils": "3.16.3",
    "@types/ember-qunit": "3.4.15",
    "@types/ember-resolver": "5.0.11",
    "@types/htmlbars-inline-precompile": "1.0.1",
    "@types/qunit": "2.19.1",
    "@types/rsvp": "4.0.4",
    "@typescript-eslint/eslint-plugin": "5.30.5",
    "@typescript-eslint/parser": "5.30.5",
    "bootstrap3": "3.3.5",
    "broccoli-asset-rev": "3.0.0",
    "ember-cli": "3.28.5",
    "ember-cli-code-coverage": "1.0.3",
    "ember-cli-dependency-checker": "3.3.1",
    "ember-cli-inject-live-reload": "2.1.0",
    "ember-cli-sass": "10.0.1",
    "ember-cli-sri": "2.1.1",
    "ember-cli-terser": "4.0.2",
    "ember-cli-typedoc": "1.1.0",
    "ember-cli-typescript-blueprints": "3.0.0",
    "ember-disable-prototype-extensions": "1.1.3",
    "ember-export-application-global": "2.0.1",
    "ember-load-initializers": "2.1.2",
    "ember-qunit": "5.1.5",
    "ember-resolver": "8.0.3",
    "ember-source": "3.28.9",
    "ember-source-channel-url": "3.0.0",
    "ember-template-lint": "3.16.0",
    "ember-try": "2.0.0",
    "eslint": "8.19.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-ember": "10.6.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-qunit": "7.3.0",
    "eslint-plugin-storybook": "0.5.13",
    "html-webpack-plugin": "5.5.0",
    "loader.js": "4.7.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.7.1",
    "qunit": "2.19.1",
    "qunit-dom": "2.0.0",
    "sass": "1.53.0",
    "standard-version": "9.5.0",
    "typescript": "4.7.4",
    "webpack": "5.73.0"
  },
  "peerDependencies": {
    "prismjs-glimmer": "^1.1.1"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "publishConfig": {
    "access": "public"
  },
  "babel": {
    "plugins": [
      [
        "@babel/plugin-proposal-private-property-in-object",
        {
          "loose": true
        }
      ]
    ]
  },
  "volta": {
    "node": "14.19.3",
    "npm": "8.11.0"
  }
}

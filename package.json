{
  "name": "@nsf-open/ember-ui-foundation",
  "version": "1.8.0",
  "description": "The starting point for Ember Application UI libraries at NSF.",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/nsf-open/ember-ui-foundation",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --aggregate-output --continue-on-error --parallel lint:*:fix",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "start": "ember serve",
    "start:with-storybook": "STORYBOOK=true npm-run-all --continue-on-error --parallel start storybook:start",
    "test:all": "npm-run-all lint test:*",
    "test": "ember test",
    "test:ember-compatibility": "ember try:each",
    "test:coverage": "COVERAGE=true ember test",
    "prepack": "ember ts:precompile",
    "postpack": "ember ts:clean",
    "release": "standard-version",
    "storybook:build": "STORYBOOK=true ember build && build-storybook",
    "storybook:start": "sleep 30s && start-storybook -p 6006 --ci"
  },
  "dependencies": {
    "@ember-decorators/component": "^6.1.1",
    "@nsf-open/ember-general-utils": "^2.0.1",
    "@popperjs/core": "^2.11.0",
    "broccoli-funnel": "^3.0.8",
    "ember-auto-import": "^1.11.3",
    "ember-cli-babel": "^7.26.6",
    "ember-cli-htmlbars": "^6.0.1",
    "ember-cli-typescript": "^4.2.1",
    "ember-compatibility-helpers": "^1.2.6",
    "ember-concurrency": "^2.2.0",
    "ember-concurrency-ts": "^0.3.1",
    "ember-decorators-polyfill": "^1.1.5",
    "ember-element-helper": "^0.5.5",
    "ember-in-element-polyfill": "^1.0.1",
    "ember-truth-helpers": "^3.0.0",
    "font-awesome": "^4.7.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@babel/eslint-plugin": "^7.16.5",
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.6.0",
    "@embroider/test-setup": "^0.48.1",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "@storybook/addon-a11y": "^6.4.19",
    "@storybook/addon-cssresources": "^6.2.9",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/ember": "^6.4.19",
    "@storybook/ember-cli-storybook": "^0.5.0",
    "@storybook/theming": "^6.4.19",
    "@types/ember__application": "^3.16.3",
    "@types/ember__array": "^3.16.4",
    "@types/ember__component": "^3.16.6",
    "@types/ember__controller": "^3.16.6",
    "@types/ember__debug": "^3.16.5",
    "@types/ember__engine": "^3.16.3",
    "@types/ember__error": "^3.16.1",
    "@types/ember__object": "^3.12.6",
    "@types/ember__polyfills": "^3.12.1",
    "@types/ember__routing": "^3.16.15",
    "@types/ember__runloop": "^3.16.3",
    "@types/ember__service": "^3.16.1",
    "@types/ember__string": "^3.16.3",
    "@types/ember__template": "^3.16.1",
    "@types/ember__test": "^3.16.1",
    "@types/ember__test-helpers": "^2.6.0",
    "@types/ember__utils": "^3.16.2",
    "@types/ember-qunit": "^3.4.15",
    "@types/ember-resolver": "^5.0.10",
    "@types/htmlbars-inline-precompile": "^1.0.1",
    "@types/qunit": "^2.11.2",
    "@types/rsvp": "^4.0.4",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "bootstrap3": "^3.3.5",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "^3.28.4",
    "ember-cli-code-coverage": "^1.0.3",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sass": "^10.0.1",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-typedoc": "^1.1.0",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-qunit": "^5.1.4",
    "ember-resolver": "^8.0.2",
    "ember-source": "^3.28.8",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^3.14.0",
    "ember-try": "^2.0.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-ember": "^10.5.9",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-qunit": "^7.2.0",
    "eslint-plugin-storybook": "^0.5.7",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "qunit": "^2.16.0",
    "qunit-dom": "^2.0.0",
    "sass": "^1.44.0",
    "standard-version": "^9.3.2",
    "typescript": "^4.5.3"
  },
  "peerDependencies": {
    "prismjs-glimmer": "^1.1.1"
  },
  "engines": {
    "node": "12.* || 14.* || >= 16"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "publishConfig": {
    "access": "public"
  },
  "babel": {
    "plugins": [
      [
        "@babel/plugin-proposal-private-property-in-object",
        {
          "loose": true
        }
      ]
    ]
  },
  "volta": {
    "node": "14.19.3",
    "npm": "8.11.0"
  }
}
